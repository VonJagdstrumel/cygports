CATEGORY=Devel
GIT_URI=https://repo.or.cz/${PN}.git
GIT_TAG=release_${PV//./_}
. ${top}/../base.cyg

src_compile() {
    cd ${S}
    lndirs
    cd ${B}
    prefix=$(__host_prefix)
    cygconf --tccdir=$prefix/lib/tcc --bindir=$prefix/bin --docdir=$prefix/share/doc --libdir=$prefix/lib
    cygmake
}
